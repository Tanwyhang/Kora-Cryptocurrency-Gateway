import requests
import json

url = "https://api.thirdweb.com/v1/transactions"
headers = {
    "Content-Type": "application/json",
    "x-secret-key": "SfEd-Vh4ymPIyoj1yX_SLM1vhz4b_b9uLfnv-BKiwRYKa-zJZGXu4lnTiMrFJPfGCT1q-BzmtzC_91ScG7r1Bw"
}

payload = {
    "chainId": 137,
    "from": "0x1234567890123456789012345678901234567890", 
    "transactions": [
        {
            "data": "0xa9059cbb000000000000000000000000742d35cc6634c0532925a3b8d43c67b8c8b3e9c6000000000000000000000000000000000000000000000000016345785d8a0000",
            "to": "0xA0b86a33E6411E3036C1C4c7E815D0a82e3F5fD6",
            "value": "0"
        }
    ]
}

